{
  "name": "Scala",
  "version": "0.1.2",
  "description": "Scala support for Visual Studio Code",
  "engines": {
    "vscode": ">=0.9.0-pre.1"
  },
  "publisher": "itryapitsin",
  "contributes": {
    "languages": [
      {
        "id": "scala",
        "aliases": [
          "Scala",
          "scala"
        ],
        "extensions": [
          ".scala"
        ],
        "configuration": "./scala.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scala",
        "scopeName": "source.scala",
        "path": "./syntaxes/scala.tmLanguage"
      }
    ]
  }
}